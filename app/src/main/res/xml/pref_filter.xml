<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:defaultValue="true"
        android:entries="@array/filter_name"
        android:entryValues="@array/filter_value"
        android:key="list_preference_filter"
        android:summary="@string/pref_sum_sort"
        android:title="@string/pref_title_sort" />

    <SwitchPreference
        android:defaultValue="true"
        android:key="switch_room_or_cursor"
        android:summary="@string/pref_sum_lib"
        android:title="@string/pref_title_lib" />

</PreferenceScreen>